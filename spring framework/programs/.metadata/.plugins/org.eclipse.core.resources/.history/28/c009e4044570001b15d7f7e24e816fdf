package com.example;

public class Retired extends User {
	public int getPension() {
		return pension;
	}

	public void setPension(int pension) {
		this.pension = pension;
	}

	private int pension;

	public Retired(int id, String name, int pension) {
		super(id, name);
		this.pension = pension;
	}
	public Retired(int id, String name) {
		super(id, name);
	}
	public Retired() {
		super();
	}
	
}
