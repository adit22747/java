package com.assignment3;

import java.util.Scanner;

public class digits {
	public static void main(String[] args) {
		System.out.println("enter value");
		Scanner sc = new Scanner(System.in);
		int number = sc.nextInt();
		int sum=0;
		while(number!=0){
			number=number%10;
			sum+=number;
			number=number/10;
		}
		System.out.println(sum);
	}
}
